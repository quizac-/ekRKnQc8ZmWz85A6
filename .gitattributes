*.c filter=git-crypt diff=git-crypt
*.cert filter=git-crypt diff=git-crypt
*.conf filter=git-crypt diff=git-crypt
*.crt filter=git-crypt diff=git-crypt
*.gpg filter=git-crypt diff=git-crypt
*.ini filter=git-crypt diff=git-crypt
*.key filter=git-crypt diff=git-crypt
*.md filter=git-crypt diff=git-crypt
*.o filter=git-crypt diff=git-crypt
*.py filter=git-crypt diff=git-crypt
*.sh filter=git-crypt diff=git-crypt
*.txt filter=git-crypt diff=git-crypt
* filter=git-crypt diff=git-crypt
.gitattributes !filter !diff
